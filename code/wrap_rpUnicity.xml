<tool id="rpUnicity" name="RP Unicity" version="1.0">
  <description>Remove duplicate pathways</description>
  <stdio>
		<regex match="ERROR:" level="error" />
		<regex match="WARNING:" level="warning" />
	</stdio>
  <command detect_errors="exit_code"><![CDATA[
		'$__tool_directory__/tool_rpUnicity.py' -input '$input' -output '$output' -server_url '$adv.server_url'
  ]]></command>
  <inputs>
    <param name="input" type="data" format="tar" label="rpSBML tar input" />
    <section name="adv" title="Advanced Options" expanded="false">
			<param name="server_url" type="text" value="http://rpunicity_rest-dev:8888/REST" label="Server URL" />
		</section>
  </inputs>
  <outputs>
    <param name="output" type="data" format="tar" label="rpSBML tar output" />
  </outputs>
  <help>
   This is the help section. Should really write something usefull here :)
  </help>
  <citations>
  </citations>
</tool>
